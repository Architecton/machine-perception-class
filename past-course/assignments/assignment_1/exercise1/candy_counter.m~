Candy_image = imread('candy.jpg');
Candy_image_gray = rgb2gray(Candy_image);

thresh = otsu(imcomplement(rgb2gray(Candy_image)));
Raw_mask = imcomplement(rgb2gray(Candy_image)) > thresh;

SE = strel('disk', 6, 4);
Processed_mask = im(Raw_mask, SE);
Res = immask(Candy_image, Processed_mask);
imagesc(Res);
